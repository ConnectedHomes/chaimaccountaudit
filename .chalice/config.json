{
    "version": "2.0",
    "app_name": "chaimaccountaudit",
    "lambda_memory_size": 256,
    "lambda_timeout": 30,
    "manage_iam_role": false,
    "iam_role_arn": "arn:aws:iam::499223386158:role/chaim-lambda-rds",
    "environment_variables": {
        "SECRETPATH": "/sre/chaim/"
    },
    "stages": {
        "dev": {
            "api_gateway_stage": "dev",
            "environment_variables": {
                "SNSTOPICARN": "arn:aws:sns:eu-west-1:168686814235:iamtool-snsreq",
                "WEBHOOK": "/sre/sgtool/dev/webhook"
            }
        },
        "prod": {
            "api_gateway_stage": "prod",
            "environment_variables": {
                "SNSTOPICARN": "arn:aws:sns:eu-west-1:168686814235:iamtool-snsreq-prod",
                "WEBHOOK": "/sre/sgtool/prod/webhook"
            }
        }
    }
}
